<h1>Listing hits</h1>

<table>
  <tr>
    <th>Clicked at</th>
    <th>Is mobile</th>
    <th>Page</th>
    <th>Agency</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @hits.each do |hit| %>
  <tr>
    <td><%= hit.clicked_at %></td>
    <td><%= hit.is_mobile %></td>
    <td><%= hit.page %></td>
    <td><%= hit.agency %></td>
    <td><%= link_to 'Show', hit %></td>
    <td><%= link_to 'Edit', edit_hit_path(hit) %></td>
    <td><%= link_to 'Destroy', hit, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Hit', new_hit_path %>
